{
  "name": "cloud-provider-client-cli",
  "description": "A PaNOSC Cloud Provider Client CLI to test the Cloud Provider APIs",
  "version": "1.0.0",
  "keywords": [
    "panosc",
    "panosc-portal"
  ],
  "main": "dist/index.js",
  "bin": {
    "cloud-provider": "./bin/run"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "npm run eslint",
    "eslint": "eslint . --ext .ts --config .eslintrc",
    "eslint:fix": "eslint . --ext .ts --config .eslintrc --fix",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "preinstall": "npx npm-force-resolutions"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "axios": "^0.19.0",
    "console-table-printer": "^1.1.27",
    "inquirer": "^7.0.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "resolutions": {
    "minimist": ">=1.2.2"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./dist/commands",
    "bin": "cloud-provider",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "instance": {
        "description": "perform instance related operations"
      },
      "image": {
        "description": "perform image related operations"
      },
      "flavour": {
        "description": "perform flavour related operations"
      },
      "node": {
        "description": "perform node related operations"
      }
    }
  },
  "types": "dist/index.d.ts"
}
