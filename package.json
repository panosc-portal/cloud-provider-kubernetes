{
  "name": "cloud-provider-kubernetes",
  "version": "1.0.0",
  "description": "Kubernetes implementation of the PaNOSC Cloud Provider microservice",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\" ",
    "posttest": "#npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t cloud-provider-kubernetes .",
    "docker:run": "docker run -p 3000:3000 -d cloud-provider-kubernetes",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "prepublishOnly": "npm run test",
    "client": "npm run build && node dist/cli/cli-client.js"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^1.5.9",
    "@loopback/cli": "^1.25.1",
    "@loopback/context": "^1.23.3",
    "@loopback/core": "^1.10.5",
    "@loopback/openapi-v3": "^1.9.10",
    "@loopback/repository": "^1.15.2",
    "@loopback/rest": "^1.21.0",
    "@loopback/rest-explorer": "^1.4.2",
    "@loopback/service-proxy": "^1.3.9",
    "@types/cron": "^1.7.1",
    "@types/dotenv": "^8.2.0",
    "@types/winston": "^2.4.4",
    "cron": "^1.7.2",
    "dotenv": "^8.2.0",
    "is-port-reachable": "^3.0.0",
    "kubernetes-client": "^8.3.5",
    "loopback-connector-postgresql": "^3.8.0",
    "loopback-connector-rest": "^3.6.0",
    "node-filter-async": "^1.1.3",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.20",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@loopback/build": "^2.0.14",
    "@loopback/eslint-config": "^4.1.5",
    "@loopback/testlab": "^1.9.4",
    "@types/express": "^4.17.2",
    "@types/node": "^10.17.5",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "body-parser": "^1.19.0",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-eslint-plugin": "^2.1.0",
    "eslint-plugin-mocha": "^6.2.2",
    "express": "^4.17.1",
    "macho": "^1.4.0",
    "source-map-support": "^0.5.13",
    "sqlite3": "^4.1.0",
    "ts-node": "^8.5.2",
    "typescript": "~3.6.3"
  }
}
